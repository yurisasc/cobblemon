{
  "timeline": [
    {
      "type": "add_holds",
      "holds": ["effects"]
    },
    {
      "type": "animation",
      "animation": ["q.move.name", "q.move.damage_category"],
      "delay": 1.2
    },
    {
      "type": "sequence",
      "condition": "q.move.damage_category != 'status' && q.effectiveness != 'immune'",
      "keyframes": [
        {
          "type": "entity_sound",
          "entityCondition": "q.effectiveness == 'normal' && q.entity.is_user == false && q.missed(q.entity.uuid) == false",
          "sound": "'cobblemon:impact.' + q.move.type",
          "pitch": 1.0,
          "volume": 1.0
        },
        {
          "type": "entity_sound",
          "entityCondition": "q.effectiveness == 'superEffective' && q.entity.is_user == false && q.missed(q.entity.uuid) == false",
          "sound": "'cobblemon:impact.' + q.move.type",
          "pitch": 1.0,
          "volume": 0.9
        },
        {
          "type": "entity_sound",
          "entityCondition": "q.effectiveness == 'resisted' && q.entity.is_user == false && q.missed(q.entity.uuid) == false",
          "sound": "'cobblemon:impact.' + q.move.type",
          "pitch": 1.0,
          "volume": 0.6
        },
        {
          "type": "entity_particles",
          "entityCondition": "q.entity.is_user == false && q.missed(q.entity.uuid) == false",
          "effect": "'cobblemon:impact_' + q.move.type",
          "locator": "target"
        },
        {
          "type": "run_action_effect",
          "actionEffect": "cobblemon:generic_effectiveness"
        }
      ]
    },
    {
      "type": "pause",
      "pause": 0.1
    },
    {
      "type": "remove_holds",
      "holds": ["effects"]
    },
    {
      "type": "pause",
      "pause": 2
    }
  ]
}